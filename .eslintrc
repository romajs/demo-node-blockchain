{
  "extends": [
    "standard"
  ],
  "env": {
    "es6": true,
  },
  "rules": {
    "arrow-parens": "error",
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "never",
        "exports": "never",
        "functions": "ignore"
      }
    ],
    "no-console": "warn",
    "no-unused-vars": "warn",
    "semi": [
      "error",
      "always"
    ]
  }
}
